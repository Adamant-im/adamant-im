  "name": "adamant-im",
  "version": "1.7.2",
  "private": true,
  "description": "Distributed chat",
  "author": "ADAMANT Tech Labs <devs@adamant.im>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "https": "vue-cli-service serve --https",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@adamant/chat": "file:packages/chat",
    "@adamant/message-formatter": "file:packages/message-formatter",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@mdi/font": "^2.6.95",
    "@stablelib/utf8": "^0.10.1",
    "abi-decoder": "^1.2.0",
    "axios": "^0.18.0",
    "b64-to-blob": "^1.2.19",
    "bignumber.js": "^4.0.4",
    "bitcore-mnemonic": "^1.2.5",
    "bytebuffer": "^5.0.1",
    "deepmerge": "^2.1.1",
    "ed2curve": "^0.2.1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "file-saver": "^1.3.8",
    "hdkey": "^0.8.0",
    "instascan": "^1.0.0",
    "js-md5": "^0.7.3",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.22.2",
    "notifyjs": "^3.0.0",
    "promise-queue": "^2.2.3",
    "register-service-worker": "^1.0.0",
    "roboto-fontface": "*",
    "semver": "^5.5.1",
    "simple-audio": "^1.0.1",
    "sodium-browserify-tweetnacl": "^0.2.3",
    "vue": "^2.5.17",
    "vue-clipboards": "^1.2.0",
    "vue-i18n": "^8.0.0",
    "vue-material": "^0.8.1",
    "vue-qrcode-component": "^2.1.1",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0",
    "web3": "0.20.6"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.3",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-rewire": "^1.2.0",
    "lorem-ipsum": "^1.0.6",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.0.1",
    "sinon": "^7.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-i18n": "^0.5.0",
    "vue-cli-plugin-vuetify": "^0.4.5",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-qrcode-component)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
